<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/buttonBackground">
        <shape
            android:shape="rectangle">

            <solid
                android:color="#fff">
            </solid>


            <corners
                android:radius="20dp">
            </corners>
        </shape>

    </item>
    <item
        android:left="10dp"
        android:right="10dp"
        android:bottom="4dp"
        android:top="4dp"
        android:gravity="center"
        android:id="@+id/buttonImage"
        android:drawable="@drawable/ic_thumb"/>
</layer-list>
